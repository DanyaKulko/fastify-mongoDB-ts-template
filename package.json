{
	"name": "fastify-ts-template",
	"version": "1.0.0",
	"main": "index.js",
	"author": "danya_kulko",
	"license": "MIT",
	"description": "",
	"repository": "https://github.com/DanyaKulko/fastify-mongoDB-ts-template",
	"keywords": ["fastify", "typescript", "mongodb", "template"],
	"scripts": {
		"dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts",
		"build": "tsc && tsc-alias",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"lint": "biome check --no-errors-on-unmatched",
		"lint:fix": "biome check --write --no-errors-on-unmatched",
		"test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {
		"@fastify/cors": "^9.0.1",
		"@fastify/helmet": "^11.1.1",
		"@fastify/jwt": "^8.0.1",
		"@fastify/swagger": "^8.14.0",
		"@fastify/swagger-ui": "^3.0.0",
		"bcrypt": "^5.1.1",
		"dotenv": "^16.4.5",
		"fastify": "^4.27.0",
		"mongoose": "^8.4.0",
		"winston": "^3.13.0",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@faker-js/faker": "^8.4.1",
		"@types/bcrypt": "^5.0.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.12",
		"@typescript-eslint/eslint-plugin": "^7.10.0",
		"@typescript-eslint/parser": "^7.10.0",
		"cross-env": "^7.0.3",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.7",
		"mongodb-memory-server": "^9.4.0",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"ts-jest": "^29.1.5",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	}
}
